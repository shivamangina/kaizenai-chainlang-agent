{
  "name": "AI Mayhem",
  "version": "1.0.2",
  "description": "AI Mayhem is a simple extension",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "manifest_version": 3,
  "author": "novobloc",
  "action":{
      "default_popup": "index.html",
      "default_title": "AI Mayhem"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}